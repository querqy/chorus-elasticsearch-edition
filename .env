# Password for the 'admin' user (at least 6 characters)
# OPENSEARCH_PASSWORD=OpenSearchRocks
OPENSEARCH_PASSWORD=admin

# Set the cluster name
CLUSTER_NAME=opensearch-cluster

# Port to expose OpenSearch HTTP API to the host
OS_PORT=9200

# Increase or decrease based on the available host memory (in bytes)
MEM_LIMIT=1073741824

#Keycloak's admin password
KEYCLOAK_ADMIN_PASSWORD=password

#MySQL's root password
MYSQL_ROOT_PASSWORD=password

#SMUI's database password
SMUI_DB_PASSWORD=password

#Grafana's admin password
GF_SECURITY_ADMIN_PASSWORD=password
